datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
}
model User {
  id      Int      @id @default(autoincrement())
  firstName    String
  lastName String
  email   String   @unique
  password    String  
  age Int 
  gender String
  location String
  phoneNumber Int

}



model Review {
  id          Int      @id @default(autoincrement())
  firstName   String
  country     String
  date        String
  ratings     String   
  comments    String
}
